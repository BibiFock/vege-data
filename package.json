{
  "name": "vege-data",
  "version": "1.0.2",
  "description": "Simple SQLLite ORM for nodejs based on sql-template-strings and sqlite",
  "main": "./lib/index.js",
  "scripts": {
    "build": "rimraf ./lib && babel src --out-dir lib",
    "install": "npm run build",
    "eslint": "eslint src/**/*.js tests/**/*.js",
    "test": "jest",
    "validate": "npm run eslint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BibiFock/vege-data.git"
  },
  "keywords": [
    "nodejs",
    "ORM",
    "SQLite"
  ],
  "author": "bibifock",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BibiFock/vege-data/issues"
  },
  "jest": {
    "testMatch": [
      "**/*.test.js"
    ],
    "modulePaths": [
      "src/",
      "tests/"
    ],
    "watchPathIgnorePatterns": [
      "build/"
    ],
    "moduleFileExtensions": [
      "js"
    ]
  },
  "homepage": "https://github.com/BibiFock/vege-data#readme",
  "dependencies": {
    "sql-template-strings": "2.2.2",
    "sqlite": "4.0.14",
    "sqlite3": "5.0.0",
    "rimraf": "3.0.2"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.4",
    "@babel/plugin-transform-async-to-generator": "7.10.4",
    "@babel/plugin-transform-runtime": "7.11.0",
    "@babel/preset-env": "7.11.0",
    "babel-eslint": "10.1.0",
    "babel-plugin-module-resolver": "4.0.0",
    "eslint": "7.7.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "23.20.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "jest": "26.4.2"
  }
}
